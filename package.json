{
  "scripts": {
    "build": "pnpm --filter skyboxx build",
    "dev": "pnpm --filter skyboxx dev",
    "lint": "pnpm --filter skyboxx lint",
    "clean": "pnpm --filter skyboxx clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && cd packages/ui && npm publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "prettier": "^3.2.5",
    "turbo": "^2.1.3"
  },
  "packageManager": "pnpm@9.12.1",
  "name": "skyboxx"
}
